{
	"name": "n8n-nodes-flowengine-data-standardize-clean",
	"version": "1.1.0",
	"description": "A production-ready n8n Community Node for cleaning and transforming data without code. Includes fuzzy deduplication, phone/name/address parsing, text extraction, formatting, type conversion, and more.",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"data-cleaning",
		"data-transformation",
		"deduplication",
		"fuzzy-matching",
		"phone-formatting",
		"email-normalization",
		"name-parsing",
		"address-parsing",
		"text-extraction",
		"flowengine"
	],
	"license": "MIT",
	"homepage": "https://github.com/Ami3466/n8n-nodes-flowengine-data-standardize-clean",
	"author": {
		"name": "FlowEngine",
		"email": "support@flowengine.cloud"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Ami3466/n8n-nodes-flowengine-data-standardize-clean.git"
	},
	"engines": {
		"node": ">=18.10",
		"pnpm": ">=9.1"
	},
	"packageManager": "pnpm@9.1.4",
	"main": "dist/nodes/DataCleaner/DataCleaner.node.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes --write",
		"lint": "eslint nodes --ext .ts --fix",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"nodes": [
			"dist/nodes/DataCleaner/DataCleaner.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^20.10.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"n8n-workflow": "^1.22.0",
		"prettier": "^3.2.0",
		"typescript": "^5.3.0"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
